{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": [
    "@huuyafwww/tsconfig-common",
    "@tsconfig/create-react-app/tsconfig.json"
  ],
  "include": ["src/index.ts"],
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "module": "ESNext",
  }
}
